<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ezen.seo0308.id.Service_ID">
<insert id="save">
	INSERT INTO id0308 
	VALUES(#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},#{param8})
</insert>
<select id="out" resultType="com.ezen.seo0308.id.Id_DTO">
	select * from id0308
</select>
<select id="idchek" resultType="com.ezen.seo0308.id.Id_DTO">
	select id from id0308 where id = #{param1}
</select>
<select id="idsearch" resultType="com.ezen.seo0308.id.Id_DTO">
	select * from id0308 where id LIKE '%'||#{param1}||'%'
</select>
<select id="ideletesearch" resultType="com.ezen.seo0308.id.Id_DTO">
	select * from id0308 where id = #{param1}
</select>
<delete id="idelete">
	DELETE FROM id0308 WHERE id = #{param1}
</delete>
<select id="ideletesearch2" resultType="com.ezen.seo0308.id.Id_DTO">
	select * from id0308 where id = #{param1} and pw = #{param2}
</select>
<update id="modify">
	UPDATE id0308 SET id =#{param1}, pw=#{param2},name=#{param3},tel=#{param4},sb=#{param5},address=#{param6},hobby=#{param7},insa=#{param8}
</update>

</mapper>